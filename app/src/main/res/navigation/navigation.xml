<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/fragmentScreenOne">
    <fragment
        android:id="@+id/fragmentScreenOne"
        android:name="com.example.custom_camera.Fragments.FragmentScreenOne"
        android:label="fragment_screen_one"
        tools:layout="@layout/fragment_screen_one" >
        <action
            android:id="@+id/action_fragmentScreenOne_to_fragmentScreenTwo"
            app:destination="@id/fragmentScreenTwo" />
    </fragment>
    <fragment
        android:id="@+id/fragmentScreenTwo"
        android:name="com.example.custom_camera.Fragments.FragmentScreenTwo"
        android:label="fragment_screen_two"
        tools:layout="@layout/fragment_screen_two" >
        <action
            android:id="@+id/action_fragmentScreenTwo_to_cameraFragment"
            app:destination="@id/cameraFragment" />
        <action
            android:id="@+id/action_fragmentScreenTwo_to_fragmentScreenThree22"
            app:destination="@id/fragmentScreenThree2" />
    </fragment>
    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.example.custom_camera.Fragments.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" >
        <action
            android:id="@+id/action_cameraFragment_to_fragmentScreenTwo"
            app:destination="@id/fragmentScreenTwo" />
        <action
            android:id="@+id/action_cameraFragment_to_fragmentScreenThree2"
            app:destination="@id/fragmentScreenThree2" />
    </fragment>
    <fragment
        android:id="@+id/fragmentScreenThree2"
        android:name="com.example.custom_camera.Fragments.FragmentScreenThree"
        android:label="fragment_screen_three"
        tools:layout="@layout/fragment_screen_three" >
        <action
            android:id="@+id/action_fragmentScreenThree2_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
</navigation>